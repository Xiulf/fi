module Js.Console =

import Core
import Js

@link_name = "console.log"
foreign log :: forall a. a -> Unit

print = print' (new "Array")

class Print t =
    print' :: JsArray -> t

member () of Print =
    print' s = log $ spread s

member (a -> r) of Print where Print r =
    print' s = fn x -> print' (push s x)