module libc =

import prim (Int8, Int16, Int32, Int64, Float32, Float64)

type C_Char    = Int8
type C_Short   = Int16
type C_Int     = Int32
type C_Long    = Int64
type C_Float   = Float32
type C_Double  = Float64
type C_Str     = [*:0]C_Char
type C_Array t = [*]t

foreign fun puts :: C_Array C_Str -> C_Int
