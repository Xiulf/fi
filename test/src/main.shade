module Main

import Fmt
import Fmt.Arg as FmtArg

@main
fn main(argc: i32, argv: [*][*:0]u8) -> i32
    -- var x = Some(22.(usize));
    var x = None.<usize>;

    match x
        Some(x): do
            write(1, "some".ptr, 4);
        end
        None: do
            write(1, "none".ptr, 4);
        end
    end

    0
end

enum Option<T>
    Some(val: T)
    None
end
