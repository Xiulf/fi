enum Option<T>
    Some(val: T)
    None
end

@lang "range"
struct Range
    lo: usize
    hi: usize

    fn next() -> Option<usize>
        if self.lo < self.hi
            self.lo += 1;
            Some(self.lo - 1)
        else
            None
        end
    end
end
