module main (main) =

import core
import core.libc (puts)
import core.intrinsics (sub_i32, ptr_offset, eq_i32)

fun main argc argv = do
    print_args argc argv
    0

fun pring_args argc argv =
    unless eq_i32 argc 0 do
        puts argv.*
        print_args (sub_i32 argc 1) (ptr_offset argv 1)

class AB a b

instance AB Bool Bool
instance AB Bool Int32
instance AB Bool a : Integer a
instance AB a Bool
instance AB a b : Integer a, Integer b
instance AB a b : Integer a
instance AB a b

