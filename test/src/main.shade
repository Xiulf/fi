module Main

import Fmt
import Fmt.Arg as FmtArg

@main
fn main(argc: i32, argv: [*][*:0]u8) -> i32
    var x = Some(22.(usize));
    -- var x = None.<usize>;
    var arg = match x
        Some(x): FmtArg.usize(&x)
        None: FmtArg.str(&"none")
    end

    print_fmt("value: %", [arg][..]);

    0
end

enum Option<T>
    Some(val: T)
    None
end
