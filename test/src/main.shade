mod core;
mod libc;

@no_mangle
fn main(argc: i32, argv: **u8) -> i32
    var x = core/vec/new();

    core/vec/push(&x, 'a');
    core/vec/push(&x, 'b');
    core/vec/push(&x, 'c');
    core/vec/push(&x, 'd');
    core/vec/push(&x, 'e');

    var d = core/vec/index(&x, 3);

    libc/write(0, d.(*u8), 1);

    var res = x.cap + x.len;

    core/vec/drop(&x);

    res.(_)
end
