module main (main) =

import core
import core.libc (puts)
import core.intrinsics (sub_i32, ptr_offset, eq_i32)

fun main argc argv = do
    print_args argc argv
    0

fun pring_args argc argv =
    unless eq_i32 argc 0 do
        puts argv.*
        print_args (sub_i32 argc 1) (ptr_offset argv 1)

