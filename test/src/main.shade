module main (main) =

import core
import core/libc (write, STDOUT_FILENO)
import core/intrinsics (unsafe, transmute)

fun main = do
    let text = get_x { x: "Hello World\n", y: 'a' }
    print text

fun print (s :: Str) = do
    let (ptr, len) = unsafe $ transmute s
    let _ = write STDOUT_FILENO ptr len

fun get_x { x, .. } = x
