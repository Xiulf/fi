extern "C" write: fn (fd: i32, buf: ref u8, count: usize) -> isize

fn main(argc: i32, argv: ref ref u8) -> i32
    println("Hello World!");
end

fn print(string: str)
    write(0, string.ptr, string.len);
end

fn println(string: str)
    write(0, string.ptr, string.len);
    write(0, 10.ref, 1);
end
