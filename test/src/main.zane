mod libc
mod util

fn main(argc: i32, argv: ref ref u8) -> i32
    sort(ref toSort)
    var i = 0
    
    while i < toSort.len
        util/print_usize(toSort[i], 10)
        i += 1
    end

    0
end

var toSort = [1, 5, 2, 12, 0, 4];

fn sort(l: ref [usize; 6])
    var i = 0;
    
    while i < l.deref.len - 1
        var j = 0;

        while j < l.deref.len - i - 1
            if l.deref[j] > l.deref[j + 1]
                var tmp = l.deref[j]

                l.deref[j] = l.deref[j + 1]
                l.deref[j + 1] = tmp
            end

            j += 1
        end

        i += 1
    end
end
