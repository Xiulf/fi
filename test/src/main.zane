main :: fn (argc: #i32, argv: ref ref #u8) -> #i32 {
    arrays()
};

arrays :: fn () -> #i32 {
    arr := [1, 2, 4, 8, 16, 32];
    sum := 0;
    i := 0;

    while i < arr.len {
        sum += arr[i];
        i += 1;
    }

    sum
};

mod c {
    puts :: extern "C" fn (string: ref #u8) -> #i32;
}
