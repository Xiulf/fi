const NULL: forall T. *T = 0.(*_);

fn(T) identity(x: T) -> T
    x
end

--| @lang "range"
struct Range
    low: usize
    high: usize
end

--| @lang "option"
enum Option(T)
    None
    Some(val: T)
end

--| @lang "type-layout"
struct TypeLayout
    size: usize
    align: usize
    stride: usize
end
